//INHERITANCE

class A // Super Class
{
    public void show()
    {
        System.out.println("class A");
    }
}
class B extends A
{
    public void disp()
    {
      System.out.println("class B");
    }
}

public class Main
{
    public static void main(String [] args)
    {
        B obj = new B();
        obj.show();
        obj.disp();
    }
}
